                  <ImageView fitHeight="30.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../Resource/servis.png" />
                     </image>
                     <effect>
                        <ColorAdjust brightness="-0.14" contrast="-0.1" hue="0.05" saturation="1.0" />
                     </effect>
                  </ImageView>
               </graphic>
               <tooltip>
                  <Tooltip text="New Servis" />
               </tooltip>
            </Button>
         </children>
      </VBox>
   </children>
   <effect>
